LOAD DATA LOCAL INFILE '/home/filab/Documents/시험코드/TSV_to_SQL/workspace_tsv2sql/claim/claims_2001.tsv' IGNORE INTO TABLE `patents_db`.`claim` CHARACTER SET utf8mb4 FIELDS TERMINATED BY '	' OPTIONALLY ENCLOSED BY '"' ESCAPED BY '"' LINES TERMINATED BY '\n' IGNORE 1 LINES SET patent_date = 0000-00-00, created_date = 0000-00-00, updated_date = 0000-00-00;
